<?xml version="1.0" encoding="UTF-8"?>

<suite name="LDC functional test" allow-return-value="true" verbose="1" parallel="tests" thread-count="6">
	<test name="bat" parallel="classes" thread-count="6">
		<classes>
			<class name="firefoxFramework.firefoxTest">
				<methods>
					<include name="BATtest" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="saveRecipe" preserve-order="true" thread-count="6">
		<classes>
			<class name="firefoxFramework.firefoxTest">
				<methods>
					<include name="saveRecipeTest" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="signUp" parallel="classes" thread-count="6">
		<classes>
			<class name="firefoxFramework.firefoxTest">
				<methods>
					<include name="signupTest" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="publishRecipe" parallel="classes" thread-count="6">
		<classes>
			<class name="firefoxFramework.firefoxTest">
				<methods>
					<include name="publishTest" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="publishArticle" parallel="classes" thread-count="6">
		<classes>
			<class name="firefoxFramework.firefoxTest">
				<methods>
					<include name="publishArticleTest" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="login" parallel="classes" thread-count="6">
		<classes>
			<class name="firefoxFramework.firefoxTest">
				<methods>
					<include name="loginTest" />
				</methods>
			</class>
		</classes>
	</test>
	
	
</suite>