<?xml version="1.0" encoding="UTF-8"?>

<suite name="LDC functional test" allow-return-value="true" verbose="1" parallel="tests" thread-count="2">
	<test name="saveRecipe" preserve-order="true" thread-count="2">
		<classes>
			<class name="firefoxFramework.FirefoxBrowser">
				<methods>
					<include name="loginTest" />
					<include name="saveRecipeTest" />
					<include name="logoutTest" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="signUp" parallel="classes" thread-count="2">
		<classes>
			<class name="firefoxFramework.FirefoxBrowser">
				<methods>
					<include name="signupTest" />
				</methods>
			</class>
		</classes>
	</test>
</suite>